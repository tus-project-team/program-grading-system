{
  "name": "website",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bun run build:wasm && bun run astro dev",
    "start": "astro dev",
    "build": "bun run build:wasm && bun run build:astro",
    "build:astro": "astro build",
    "build:wasm": "cargo component build --release --package tools && cp ../target/wasm32-wasip1/release/tools.wasm generated/tools/tools.wasm && bun run jco transpile generated/tools/tools.wasm -o generated/tools",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:write": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "format": "prettier --ignore-path .gitignore --check .",
    "format:write": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "@astrojs/react": "^4.0.0",
    "@astrojs/starlight": "^0.30.0",
    "@astrojs/starlight-tailwind": "^3.0.0",
    "@astrojs/tailwind": "^5.1.3",
    "@bytecodealliance/jco": "1.8.1",
    "@bytecodealliance/preview2-shim": "^0.17.1",
    "@monaco-editor/react": "^4.6.0",
    "@rollup/browser": "^4.28.1",
    "astro": "^5.0.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.13.1",
    "justd-icons": "^1.9.1",
    "react": "^19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "eslint": "9.17.0",
    "eslint-config-flat-gitignore": "0.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.3.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-unicorn": "56.0.1",
    "globals": "15.13.0",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1",
    "vite-tsconfig-paths": "5.1.4",
    "wireit": "0.14.9"
  },
  "trustedDependencies": [
    "astro",
    "sharp"
  ]
}
