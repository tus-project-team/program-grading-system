    };
    m.minus = m.sub = function(e) {
      var t, r, n, i, o, s, a, l, u, c2, p2, d, f = this, g2 = f.constructor;
      if (e = new g2(e), !f.d || !e.d) return !f.s || !e.s ? e = new g2(NaN) : f.d ? e.s = -e.s : e = new g2(e.d || f.s !== e.s ? f : NaN), e;
