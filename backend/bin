          p2 = u[n] < d[n];
          break;
        }
        o = 0;
      }
      for (p2 && (t = u, u = d, d = t, e.s = -e.s), s = u.length, n = d.length - s; n > 0; --n) u[s++] = 0;
      for (n = d.length; n > o; ) {
        if (u[--n] < d[n]) {
          for (i = n; i && u[--i] === 0; ) u[i] = ge - 1;
          --u[i], u[n] += ge;
        }
        u[n] -= d[n];
      }
      for (; u[--s] === 0; ) u.pop();
      for (; u[0] === 0; u.shift()) --r;
      return u[0] ? (e.d = u, e.e = nn(u, r), w ? y(e, a, l) : e) : new g2(l === 3 ? -0 : 0);
    };
    m.modulo = m.mod = function(e) {
