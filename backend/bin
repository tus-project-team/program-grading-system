      });
    }
    function Sp(e, t, r) {
      let n = t.arguments.getDeepSubSelectionValue(e.selectionPath)?.asObject();
      if (n) {
        let i = n.getField("omit")?.value.asObject();
        if (i) {
          Ap(e, t, i);
          return;
        }
        if (n.hasField("select")) {
          Ip(e, t);
          return;
        }
      }
